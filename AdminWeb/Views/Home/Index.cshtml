@{
    ViewData["Title"] = "Home Page";
}


@section jsFooter{

    }
    <div style="padding:3%">
        <div style="display:flex">
            <div style="width:70%">
                <div class="js__canvas">
                </div>
            </div>
            <div style="display:flex">
                <div style="width:25%;background-color:white;padding:5%;">
                    <span>Năm</span>
                    <select class="take-year">
                        @foreach (var item in ViewBag.Years)
                    {
                        <option data-id="@item.Text" href="#">@item.Text</option>
                    }
                    </select>
                    <span>Tháng</span>
                    <select class="take-month">
                        <option value="" selected disabled hidden>Chọn</option>
                        <option value="">Trống</option>
                        <option data-id="1" href="#">1</option>
                        <option data-id="2" href="#">2</option>
                        <option data-id="3" href="#">3</option>
                        <option data-id="4" href="#">4</option>
                        <option data-id="5" href="#">5</option>
                        <option data-id="6" href="#">6</option>
                        <option data-id="7" href="#">7</option>
                        <option data-id="8" href="#">8</option>
                        <option data-id="9" href="#">9</option>
                        <option data-id="10" href="#">10</option>
                        <option data-id="11" href="#">11</option>
                        <option data-id="12" href="#">12</option>
                    </select>
                    <input class="change-year" type="submit" value="Tìm kiếm" />
                </div>
                <div style="width:75%;" class="log-to-you">

                </div>
            </div>
         </div>
    </div>
<div style="display:flex">
    <div style="width:70%">
    </div>
    <div style="width:30%;background-color:red">
    </div>
</div>
